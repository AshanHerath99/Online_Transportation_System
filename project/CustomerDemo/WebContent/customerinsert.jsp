<center><div class="col-lg-4  col-md-6 header-right">
							<h4 class="pb-30">USER SIGN UP</h4>
							<form class="form" action="insert" method="post">
							    <div class="from-group">
							    	<input class="form-control txt-field" type="text" name="name" placeholder="Your name"  onfocus="this.placeholder = ''" onblur="this.placeholder = 'Your name'">
							    	<input class="form-control txt-field" type="email" name="email" placeholder="Email address" onfocus="this.placeholder = ''" onblur="this.placeholder = 'Your Email address'">
							    	<input class="form-control txt-field" type="tel" name="phone" placeholder="Phone number" onfocus="this.placeholder = ''" onblur="this.placeholder = 'Phone number'">
							     <input class="form-control txt-field" type="un" name="uid" placeholder="Username" onfocus="this.placeholder = ''" onblur="this.placeholder = 'Username'">
                                     <input class="form-control txt-field" type="password" name="psw" placeholder="password" onfocus="this.placeholder = ''" onblur="this.placeholder = 'Password'">
                                <input class="form-control txt-field" type="password" name="re_password" placeholder="re_password" onfocus="this.placeholder = ''" onblur="this.placeholder = 'Repeat your Password'">        
                                 </div>								
							   					    
							   					    
							    <div class="form-group">

							            <button class="btn btn-default btn-lg btn-block text-center text-uppercase">Sign Up</button>

							    </div>
							</form>
						</div>
                           	</div>
				</div></center>					

	

